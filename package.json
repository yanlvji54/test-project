    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sonarjs": "^1.2.0",
    "eslint-plugin-typescript": "^0.14.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.4",
    "lovable-tagger": "^1.1.7",
    "postcss": "^8.4.35",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^3.2.5",
